{
	"name": "david63/langgroup",
	"type": "phpbb-extension",
	"description": "An extension to automatically move a user to a group based on their language choice.",
	"version": "3.1.0-RC1",
	"time": "2020-08-06",
	"keywords": [
		"phpbb",
		"extension",
		"language",
		"group",
		"profile"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "David Wood",
			"role": "Developer"
		}
	],
   "require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "Language groups",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
		},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "lang_group_version_file.json",
			"ssl": false
    	}
	}
}
